{% extends "base.njk" %}
{% block title %}{{ tag.name }} - ResearchRoomies{% endblock %}
{% block content %}
<h2>{{ tag.name }} Conferences</h2>
<ul>
  {% for conf in conferences %}
  <li>
    <a href="/conference/{{ conf.slug }}">
      <strong>{{ conf.name }}</strong><br />
      {{ conf.start_time | date("MMM d") }} - {{ conf.stop_time | date("MMM d") }}
    </a>
  </li>
  {% endfor %}
</ul>
{% endblock %}